generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Services {
  id       Int    @id @default(autoincrement())
  text     String
  publicId String
  imageUrl String
}

model WeddingTypes {
  id       Int    @id @default(autoincrement())
  publicId String
  imageUrl String
  text     String
}

model WeddingGallery {
  id       Int    @id @default(autoincrement())
  publicId String
  imageUrl String
}

model WeddingTrends {
  id       Int    @id @default(autoincrement())
  publicId String
  imageUrl String
}
